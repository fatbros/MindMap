</head>
<body>
	 <header>
        <div id="logo">
            <img src="/images/7-2.png" alt="logo">
        </div>
    </header>
    <!-- /header -->

    <div id="mainWrap">

        <div id="side">
        	<h4 class="navHeader">
                    USER
            </h4>
            <div class="userInfo">
                <!-- /.navHeader -->
                <div class="userPic">
                    <div class="over_blue">
                        <div class="over_white">
                            <img src="/images/user.png" alt="user">
                        </div>
                        <!-- /#over_white -->
                    </div>
                    <!-- /#over_blue -->
                </div>
                <!-- /#userPic -->
                <div class="userName"></div>
                <!-- /#userName -->
            </div>
            <!-- /#userInfo -->
            <h4 class="navHeader">
                    MENU
            </h4>
            <nav>
                <ul>
                    <li id="brunch">Brunch Edit</li>
                    <li id="image">Image Edit</li>
                    <li id="canvas">Canvas</li>
                </ul>
            </nav>
        </div>
        <!-- /#side -->

        <div id="main">

            <div id="loginWrap">
                <div id="loginForm">
                    <div id="form_top">
                        <div id="form_pic">
                            <img src="/images/user.png" alt="user">
                        </div>
                        <input type="text" id="form_name" placeholder="Your Name">
                        </input>
                        <div class="clear"></div>
                    </div>
                    <!-- /#form_top -->
                    <div id="form_bottom">
                        <div id="loginButton_box">
                            <div id="loginButton">
                                Login
                            </div>
                        </div>
                    </div>
                    <!-- /#form_bottom -->
                </div>
                <!-- /#loginForm -->

                <div id="loginForm_start">
                    
                </div>
                <!-- /#loginForm_start -->
            </div>
            <!-- /#login_wrap -->

            <div id="canvas_area">
                <div id="canvas_topBar">
                    <img src="/images/close.png" alt="close" id="close_btn">
                </div>
                <!-- /#canvas_topBar -->
                <canvas width="500px" height="400px" id="draw_canvas">
                </canvas>
                <div id="canvas_bottomBar">
                    <div id="select_colorBox">
                        <div id="colorBox_top">
                            <div class="white">
                                <div id="pink"></div>
                            </div>
                            <div class="white">
                                <div id="blue"></div>
                            </div>
                            <div class="white">
                                <div id="green"></div>
                            </div>
                            <div class="white">
                                <div id="yellow"></div>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <!-- /#colorBox_top -->

                        <div id="colorBox_bottom">
                            <div class="white">
                                <div id="orange"></div>
                            </div>
                            <div class="white">
                                <div id="brown"></div>
                            </div>
                            <div class="white">
                                <div id="gray"></div>
                            </div>
                            <div class="white">
                                <div id="black"></div>
                            </div>
                            <div class="clear"></div>
                        </div>
                        <!-- /#colorBox_bottom -->

                    </div>
                    <!-- /#select_colorBox -->
                    <div id="eraser">
                        
                    </div>
                    <!-- /#eraser -->
                    <div class="clear"></div>
                </div>
                <!-- /#canvas_bottomBar -->
            </div>
            <!-- /#canvas_area -->
        </div>
        <!-- /#main -->
        <div id="other_user_login">
            <h4 class="navHeader">
                LOGIN
            </h4>
        </div>
        <!-- /#other_user_login -->
    </div>
    <!-- /#mainWrap -->
	<script type="text/javascript">
		//var svg_selector = Raphael('svg','100%','100%');
		var element = [];
		var access = {};
	</script>
</body>
</html>

